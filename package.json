{
  "name": "js13kserver",
  "version": "1.0.2",
  "description": "js13kgames game jam server",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "start": "node index.cjs",
    "start:dev": "concurrently \"rollup --watch --config rollup.config.mjs\" \"nodemon index.cjs\"",
    "test": "mocha test"
  },
  "keywords": [
    "js13kgames",
    "server"
  ],
  "engines": {
    "node": ">=8.*"
  },
  "nodemonConfig": {
    "ext": "js,mjs,ts,json,css,scss,sass,html,pug"
  },
  "dependencies": {
    "archiver": "^5.3.1",
    "body-parser": "^1.20.1",
    "chalk": "^4.1.2",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mysql2": "^3.0.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "socket.io": "^4.5.4",
    "sqlite3": "^5.1.4"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.27.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "roadroller": "^2.1.0",
    "rollup": "^3.10.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "prettier": {
    "printWidth": 120,
